{
  "name": "hackium",
  "version": "2.0.1",
  "description": "Hackium is a browser, a CLI tool, and a framework for web power users.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsoverson/hackium"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "compile": "tsc --declaration",
    "clean": "rimraf dist",
    "copyfiles": "copyfiles src/**/*.js dist",
    "precompile": "npm run clean && mkdir dist && npm run copyfiles",
    "prewatch": "npm run compile",
    "prepublishOnly": "npm run compile",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "watch": "tsc -w",
    "test": "mocha -b --exit"
  },
  "bin": {
    "hackium": "./bin/hackium"
  },
  "files": [
    "dist",
    "bin",
    "client",
    "extensions",
    "pages",
    "src",
    "README.md",
    "tsconfig.json",
    "LICENSE.md"
  ],
  "directories": {
    "dist": "./dist",
    "src": "./src"
  },
  "keywords": [
    "hackium",
    "puppeteer",
    "browser",
    "automation",
    "chromium",
    "chrome",
    "web pages",
    "shift-refactor",
    "reverse engineering",
    "hacking",
    "pentesting"
  ],
  "author": "Jarrod Overson <jsoverson@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/debug": "4.1.5",
    "@types/seedrandom": "2.4.28",
    "chalk": "4.0.0",
    "chokidar": "3.4.0",
    "d3-ease": "1.0.6",
    "debug": "4.1.1",
    "devtools-protocol": "0.0.777489",
    "find-root": "1.1.0",
    "import-fresh": "3.2.1",
    "inquirer": "7.2.0",
    "puppeteer": "5.2.1",
    "puppeteer-extensionbridge": "1.1.0",
    "puppeteer-interceptor": "2.1.0",
    "seedrandom": "3.0.5",
    "shift-parser": "7.0.3",
    "shift-printer": "1.0.1",
    "supports-color": "7.1.0",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@jsoverson/test-server": "^1.3.3",
    "@types/chai": "4.2.11",
    "@types/chai-spies": "^1.0.1",
    "@types/chokidar": "2.1.3",
    "@types/d3-ease": "1.0.9",
    "@types/find-root": "1.1.1",
    "@types/inquirer": "6.5.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.2",
    "@types/rimraf": "3.0.0",
    "@types/serve-handler": "6.1.0",
    "@types/tween.js": "18.5.1",
    "@types/yargs": "15.0.4",
    "chai": "4.2.0",
    "chai-spies": "^1.0.0",
    "copyfiles": "2.3.0",
    "mocha": "10.2.0",
    "mock-stdin": "1.0.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "ts-node": "8.9.0",
    "typescript": "3.8.3"
  }
}
